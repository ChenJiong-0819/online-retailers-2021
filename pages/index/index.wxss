.musicControl{
  position: fixed;
  bottom: 50%;
  left: 0;
  width: 80rpx;
  height: 80rpx;
  background-color: #fff;
  display: flex;
  border-radius: 20rpx;
  color: var(--themeColor);
  justify-content: center;
  align-items: center;
  background-color: transparent;
}
.musicControl text{
  font-size: 60rpx;
}